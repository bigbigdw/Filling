<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.filling">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_filling"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity2"
            android:label="@string/title_activity_main2"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Login.Activity_Login"
            android:theme="@style/LoginTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Login.Activity_CardPW"
            android:label="@string/page_cardPW"
            android:theme="@style/FindTheme" />
        <activity
            android:name=".Login.Activity_Find"
            android:label="@string/Find_Title"
            android:theme="@style/FindTheme" />
        <activity
            android:name=".Login.Activity_Register"
            android:theme="@style/LoginTheme" />
        <activity
            android:name=".Login.Activity_Splash"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen" />
    </application>

</manifest>